{
  "compilerOptions": {
    "noEmit": true,
    "sourceMap": true,
    "pretty": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "strict": true,
    "module": "commonjs",
    "target": "esnext",
    "jsx": "react",
    "baseUrl": "src",
    "paths": {
      "common/*": ["components/common/*"],
      "modules/*": ["components/modules/*"],
      "pages/*": ["pages/*"],
      "templates/*": ["templates/*"],
      "images/*": ["assets/images/*"],
      "fonts/*": ["assets/fonts/*"],
      "vectors/*": ["assets/vectors/*"],
      "css": ["assets/css/*"],
      "services/*": ["services/*"],
      "ducks": ["ducks/index"],
      "ducks/*": ["ducks/*"],
      "selectors": ["selectors/index"],
      "selectors/*": ["selectors/*"],
      "styles/*": ["styles/*"],
      "styles": ["styles/index"],
      "types": ["types/index"]
    },
    "typeRoots": ["node_modules/@types"],
    "lib": ["dom", "es2017"],
    "plugins": [
      {
        "name": "ts-graphql-plugin",
        "schema": "src/__generated__/gatsby-schema.graphql",
        "tag": "graphql"
      }
    ]
  }
}